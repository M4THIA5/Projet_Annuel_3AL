<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          fx:controller="fr.laporteacote.javawebscraper.ResultController"
          hgap="10" vgap="10" styleClass="page">
    <TextArea fx:id="text" GridPane.columnIndex="0" GridPane.rowIndex="0" wrapText="true" GridPane.hgrow="ALWAYS"/>
    <Button onAction="#saveValueClipBoard" fx:id="copyBtn" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.hgrow="ALWAYS" text="Copy"/>
    <Button onAction="#saveInFile" fx:id="saveBtn" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.hgrow="ALWAYS" text="Save"/>
</GridPane>
